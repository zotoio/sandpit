
{
  "pull_request": {
    "av": "fjsskdmsd",
    "agentGroup": "av",
    "tasks": [
      {
        "executor": "Http",
        "context": "avtest",
        "options": {
          "har": {
            "url": "https://httpbin.org/headers",
            "method": "GET",
            "headers": [
              {
                "name": "accept",
                "value": "application/json"
              }
            ]
          },
          "validator": {
            "type": "bodyJson",
            "objectCheckRef": "headers.Host",
            "objectCheckVal": "httpbin.org"
          }
        }
      },
      {        
        "executor": "DockerSonarPullRequest",
        "context": "Sonar test",
        "options": {
          "BUILD_TYPE": "nodejs" 
        },
        "tasks": [
          {
            "executor": "Docker",
            "context": "run ls in latest alpine2a",
            "options": {
              "image": "alpine:latest",
              "command": "/bin/ls -ltra /bin/ls"
            }
          }
        ]
      ]
   }    
}

